{
  "name": "@lazarljubenovic/vis-ng",
  "version": "0.1.2",
  "license": "MIT",
  "angular-cli": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/lazarljubenovic/ng-vis"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tmp && mkdir dist && mkdir tmp && rimraf 'src/**/*.ngfactory.ts'",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "npm run clean && npm run ngc:es5 && npm run ngc:es2015 && npm run ts:copy && npm run bundle:umd && npm run packagejson",
    "ngc:es5": "ngc -p tsconfig.json",
    "postngc:es5": "cp -R tmp/es5/src/* dist/ && rimraf src/**/*.ngfactory.ts",
    "ngc:es2015": "ngc -p tsconfig.es2015.json",
    "postngc:es2015": "cp -R tmp/esm/src dist/esm && rimraf src/**/*.ngfactory.ts",
    "ts:copy": "mkdir -p dist/ts && cp -R src/* dist/ts",
    "bundle:umd": "rollup -c rollup.config.js",
    "packagejson": "node ./scripts/create-package-json.js"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^2.2.0",
    "@angular/compiler": "^2.2.0",
    "@angular/compiler-cli": "^2.2.0",
    "@angular/core": "^2.2.0",
    "@angular/forms": "^2.2.0",
    "@angular/http": "^2.2.0",
    "@angular/platform-browser": "^2.2.0",
    "@angular/platform-browser-dynamic": "^2.2.0",
    "@angular/platform-server": "^2.2.0",
    "@angular/router": "^3.1.0",
    "@angular/upgrade": "2.2.0",
    "core-js": "^2.4.1",
    "graphlib": "^2.1.1",
    "rollup": "^0.36.3",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "vis": "^4.17.0",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/graphlib": "^2.1.2",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "1.0.0-beta.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
